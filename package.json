{
  "name": "movie-api-project",
  "version": "1.0.0",
  "description": "Movie search project based on OMDB Api.",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "start:app": "cd movie-search-app && npm install && ng serve",
    "build:app": "cd movie-search-app && ng build",
    "dc:start": "docker-compose up",
    "dc:start-bg": "docker-compose up -d",
    "dc:stop": "docker-compose stop",
    "dc:clean": "docker-compose down",
    "dc:build": "docker-compose build --no-cache",
    "db:cli:mongo": "docker exec -it movie-api-mongodb sh -c \"mongo --host localhost --username username --password password\"",
    "logs:mongo": "(docker-compose logs --follow mongo)",
    "logs:api": "(docker-compose logs --follow app)"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@elastic/elasticsearch": "^8.7.0",
    "@types/elasticsearch": "^5.0.40",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.1",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^20.1.7",
    "axios": "^1.4.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "elasticsearch": "^16.7.3",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "jest": "^29.5.0",
    "mongodb": "^5.5.0",
    "mongoose": "^7.1.1",
    "supertest": "^6.3.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/supertest": "^2.0.12",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3"
  }
}